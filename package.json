    "ua-parser-js": "^0.7.24",
    "vue": "^2.5.21",
    "vue-lazyload": "^1.2.6",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0",
